<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/style_folder/header.css">
  <link rel="stylesheet" href="/style_folder/schedules.css">
  <title><%= title %></title>
</head>
<body>
  <%- include('../_header') %>
  <h1>Schedules</h1>
  <div class="container">
    <% years.forEach(year => { %>
      <div class="tile">
        <a href="/schedules/<%= year %>"><%= year %></a>
      </div>
    <% }) %>
  </div>
</body>
</html>